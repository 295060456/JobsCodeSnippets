	<key>IDECodeSnippetContents</key>
	<string>-(JobsTextField *)textField{
    if(!_textField){
        @jobs_weakify(self)
        _textField = makeJobsTextField(^(__kindof JobsTextField * _Nullable data) {
            @jobs_strongify(self)
            data.backgroundColor = JobsClearColor;
            data.leftViewByOutLineOffset = JobsWidth(4);
            data.rightViewByTextFieldOffset = JobsWidth(8);
            // 只针对真实的textField配置
            data.leftView = jobsMakeImageView(^(__kindof UIImageView * _Nullable imageView) {
                imageView.image = JobsIMG(@"🔍");
            });
            data.realTextFieldBgCor = JobsCor(@"F5F5F5");
            data.titleCor = JobsCor(@"#788190");
            data.leftViewByOutLineOffset = JobsWidth(4);
            data.leftViewByTextFieldOffset = JobsWidth(10);
            data.rightViewByTextFieldOffset = JobsWidth(4);
            data.rightViewByOutLineOffset = JobsWidth(14);
            data.returnKeyType = UIReturnKeyDefault;
            data.keyboardAppearance = UIKeyboardAppearanceDefault;
            data.keyboardType = UIKeyboardTypeDefault;
            data.leftViewMode = UITextFieldViewModeNever;
            data.rightViewMode = UITextFieldViewModeNever;
            data.textFieldPlaceholder = JobsInternationalization(@"Search Games");
            data.placeholderColor = JobsCor(@"#788190");
            data.placeholderFont = UIFontWeightRegularSize(12);
            data.attributedPlaceholder = nil;
            data.layoutSubviewsRectCorner = UIRectCornerAllCorners;
            data.layoutSubviewsRectCornerSize = CGSizeMake(JobsWidth(8), JobsWidth(8));
            // 真实的textField，输入回调（每次输入的字符），如果要当前textField的字符，请取值textField.text
            [data actionObjBlock:^(id  _Nullable data) {
                JobsLog(@"ddf = %@",data);
            }];
            self.addSubview(data);
            [data mas_makeConstraints:^(MASConstraintMaker *make) {
                make.left.right.equalTo(self.announcementView);
                make.top.equalTo(self.announcementView.mas_bottom).offset(JobsWidth(5));
                make.height.mas_equalTo(JobsWidth(22));
            }];data.jobsRichViewByModel(nil);
        });
    }return _textField;
}</string>
